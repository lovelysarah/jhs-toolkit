{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["@remix-run/eslint-config", "@remix-run/eslint-config/node"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "react"],
    "rules": {
        "@typescript-eslint/consistent-type-imports": 1
    }
}
